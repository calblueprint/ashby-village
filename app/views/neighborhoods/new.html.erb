<h1>Create New</h1>
  <% if @repeat_error %>
  	<p> This neighborhood already exists! <p>
  <% end %>
  <%= form_for @neighborhood do |f| %> 
    Name: <%= f.text_field :title %> 
    Description: <%= f.text_field :description %>
    Location: <%= f.text_field :location %>

    <%= f.submit %> 
  <% end %>


