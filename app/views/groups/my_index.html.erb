<div class="mobile-margins">
  <div class="my-events row">
    <h3>My Groups</h3>
  </div>
  <%= render "groups/group_tiles" %>
  <% if @groups.count == 0 %>
    <div class="event-tile-margin">
      <div class="no-events row">
        <h5>You have not joined any groups yet!</h5>
      </div>
    </div>
  <% end %>
</div>
