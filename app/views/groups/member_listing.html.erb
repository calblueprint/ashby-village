<div class="listing-member row">
    <% @group.users.each do |user| %>
        <div class="large-6 columns">
            <div class="listing-member-tile row">
              <div class="listing-member-display large-3 columns"><% if user.photo %>
                <%= image_tag(user.photo(:thumb)) %>
              <% end %></div>
              <div class="large-9 columns">
                <h5><%= link_to user.decorate.full_name, user %></h5>
                <h6><%= user.phone %></h6>
              </div>
            </div>
        </div>
    <% end %>
</div>
